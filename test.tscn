script/source = "@tool
class_name TerraCurve extends Line2D

@export var point1 : Vector2
@export var point2 : Vector2
@export_range(1, 10) var strength_factor : float = 2 : 
	set(val):
		strength_factor = val
		_redraw()
var strength : int
@export var redraw : bool = false :
	set(val):
		_redraw()
	get:
		return false

var curve : Curve2D = Curve2D.new()

func _redraw():
	strength = (point2.x - point1.x) / strength_factor
	curve.clear_points()
	curve.add_point(point1, Vector2.ZERO, Vector2(strength, 0))
	curve.add_point(point2, -Vector2(strength, 0), Vector2.ZERO)
	points = curve.get_baked_points()
	queue_redraw()
"

